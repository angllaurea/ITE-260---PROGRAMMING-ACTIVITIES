create_password=input("Create password: ")

while True:
    user_password = input("Enter password: ")
    if user_password != create_password:
        print("Incorrect password")
    else:
        print("Password correct. Access granted.")
        break

while True:
    ques=input('1. The _____ statement is used inside the loop to exit out of the loop.\nA. Pass\nB. Continue \nC. break \nAnswer: ').upper()
    if ques=='A':
        print('Incorrect\n ')
        continue
    elif ques=='B':
        print('Incorrect\n ')
        continue
    elif ques=='C':
        print('Correct\n')
        break
    else:
        print('Invalid Choice\n')
        continue
   
while True:
    ques=input('2. The ____ statement is considered a no-operation statement.?\nA. Pass\nB. Continue \nC. break \nAnswer: ').upper()
    if ques=='A':
        print('Correct\n ')
        break
    elif ques=='B':
        print('Incorrect\n ')
        continue
    elif ques=='C':
        print('Incorrect\n')
        break
    else:
        print('Invalid Choice\n')
        continue

